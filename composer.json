{
  "name": "kaktus/crouton",
  "type": "library",
  "description": "Crouton lets you easily manipulate etc/cron",
  "keywords": [
    "kaktus",
    "crouton",
    "cron"
  ],
  "homepage": "https://github.com/ddymko/crouton",
  "license": "MIT",
  "authors": [
    {
      "name": "David Dymko",
      "email": "david@dymko.me",
      "homepage": "http://www.kaktus.io"
    }
  ],
  "require": {
    "php" : "~5.6|~7.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "~4.0||~5.0"
  },
  "autoload": {
    "psr-4": {
      "Kaktus\\Crouton\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kaktus\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  }
}